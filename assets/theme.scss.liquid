  @include media-query($widescreen) {
    margin-left: -135px;
    width: 135px;
    .share span {
      display: block;
    }
  }

  @include media-query($small) {
    position: fixed;
    right: 0;
    bottom: 0;
    left: 0;
    height: auto;
    top: auto !important;
    z-index: 1000;

    .sharing {
      position: static;
      padding: 10px 0;
      background: #fff;
      box-shadow: 0 -2px 5px rgba(0, 0, 0, .15);
      display: flex;
      align-items: center;
      justify-content: space-between;

