/*=============== Image Columns ===============*/

.homepage-columns{
  display: flex;
  align-items: stretch;
}
.column__block{
  position: relative;
  overflow: hidden;
  float: none;

  .flex--centered{
    padding: 15px;
  }
}
.homepage-columns {
  .column__block {
    height: 100%;
  }

  &.image-height {
    .column__block{
      height: auto;
    }

    &.auto-height {
      .column__block{
        height: 33vw;
        display: flex;
        align-items: center;
        justify-content: center;
      }
    }
