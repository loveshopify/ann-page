/**
 * Js dropdown
 */


.js-dropdown {
  padding: 0;
  text-align: left;
  min-height: 0;
  border: 0;
  position: relative;
  overflow: visible;
  display: block;
}
.js-dropdown__label {
  display: block;
  padding: 15px;
  cursor: pointer;
  position: relative;
  text-transform: capitalize;
  border-bottom: 1px solid $color_site_border_light;
}
.js-dropdown__label::after {
  font-family: 'Icons';
  content: '\e02d';
  position: absolute;
  right: 15px;
  font-size: 75%;
  top: 50%;
  z-index: 10;
  @include prefix(transform, translateY(-50%), webkit moz ms o spec);
}
.js-dropdown__list {
  padding: 0;
  position: absolute;
  z-index: 99;
  background: $color_bg_secondary;
  top: 100%;
  left: 0;
  right: 0;
  max-height: 0;
  overflow: hidden;
  transition: max-height 0.5s ease-in;
}
.js-dropdown__list.visible {
  max-height: 400px;
  overflow-y: scroll;
}
.js-dropdown__list li {
  display: block;
  border-top: 1px solid $color_site_border;
  padding: 15px;
  cursor: pointer;
  &:hover {
    color: $color_link_hover;
    background: $color_bg;
  }

  &.active {
    a {
      box-shadow: none;
    }
  }
}

/**
 * Collections
 */
.template-collection .container {
  background-color: rgba(255,255,255,0);
}

.collection__nav,
.product-grid {
  background-color: $color_bg;
}

.filter-tag {
  position: relative;
  margin: 0;
  padding: 0;
  overflow: visible;
  display: block;
  padding: 0 30px;
  min-height: 45px;
  text-align: center;

  ul {
    position: relative;
    margin: 0;
    padding: 0;
    list-style: none;
    text-align: left;
  }

  li {
    position: relative;
    margin: 0;
    padding: 0;
    display: inline-block;
    text-decoration: none;
    color: $color_text_light;
    font-weight: $font-weight-body-bold;

    a {
      font-family: $font-stack-heading;
      text-decoration: none;
      color: $color_text_light;
      -webkit-transition: color 0.15s ease-in;
      -moz-transition: color 0.15s ease-in;
      -ms-transition: color 0.15s ease-in;
      -o-transition: color 0.15s ease-in;
      transition: color 0.15s ease-in;
      padding: 15px;
      display: block;
    }

    &.back a {
      margin-left: 0;
    }

    & a:hover,
    &.active a {
      text-decoration: none;
      color: $color_text_dark;
    }

    &.active a {
      box-shadow: 0 2px $color_text_dark;
    }
  }
}

.collection__filters {
  h3 {
    padding-top: 5px;
    padding-bottom: 5px;
    margin-bottom: 0;
  }

  &.has-tags-expanded {
    .collection__filter-title:not(.is-expanded) {
      opacity: 0.5;
    }
  }
  @include media-query ($medium-up) {
    .collection__filter-tag--number + .collection__filter-tag--default {
      margin-left: 20px;
    }
  }
}

.collection__filters-wrapper {
  padding: 0 120px;
  @include media-query ($small) {
    padding: 0 20px;
  }

  & + .collection__filters-wrapper {
    border-top: 1px solid $color_site_border;
  }
}

.collection__filters-nav {
  position: relative;
  display: flex;
  justify-content: flex-end;
  padding: 8px 0 9px;
  margin: 10px 0;

  .reset-filters {
    position: absolute;
    top: 0;
    left: 0;
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    font-size: $font-size-base;

    .icon {
      fill: #fff;
    }
  }
  @include media-query ($small) {
    display: block;
    text-align: center;

    .reset-filters {
      position: static;
      margin-bottom: 10px;
    }
  }
}

.filter-tag li a {
  font-family: $font-stack-heading;
  font-style: $font-style-heading;
  text-decoration: none;
  color: $color_text_light;
  -webkit-transition: color 0.15s ease-in;
  -moz-transition: color 0.15s ease-in;
  -ms-transition: color 0.15s ease-in;
  -o-transition: color 0.15s ease-in;
  transition: color 0.15s ease-in;
  padding: 15px;
  display: block;
}

.collection__filter {
  position: static;
  padding: 10px 0;
  display: none;

  &.is-expanded {
    .collection__filter-tag {
      animation: 0.3s ease-in-out both fade-in;
      @for $i from 1 through 100 {
        &:nth-child(#{$i}) {
          animation-delay: $i * (.03s);
        }
      }
    }
  }
}
// Filter tags animation
@keyframes fade-in {
  0% {
    @include prefix(transform, scale(0), webkit moz ms o spec);
  }

  100% {
    @include prefix(transform, scale(1), webkit moz ms o spec);
  }
}

.collection__filter-title {
  display: inline-block;
  padding-right: 20px;
  margin: 0 25px;
  position: relative;
  cursor: pointer;
  color: $color_text;
  text-transform: uppercase;
  letter-spacing: 0.1em;

  .icon {
    position: absolute;
    top: 2px;
    right: 0;
    width: 6px;
    height: 14px;
    transition: transform 0.3s ease, opacity 0.3s ease;
    @include prefix(transform, rotate(90deg), webkit moz ms o spec);
  }

  &.is-expanded {
    .icon {
      @include prefix(transform, rotate(-90deg), webkit moz ms o spec);
    }
  }
}

.collection__filter-tags {
  margin: 0;
  width: 100%;
  list-style: none;
  background: $color_bg;
  display: flex;
  justify-content: flex-end;
  flex-wrap: wrap;
  @include media-query ($medium-down) {
    display: block;
    text-align: center;
  }
}

.collection__filter-tag {
  .swatch {
    flex: 0;
  }

  button {
    color: $color_text;
    background: transparent;
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    font-size: $font-size-base;
    text-transform: capitalize;

    &:active,
    &:hover {
      transform: none;
      box-shadow: none;
    }
  }
}

.collection__filter-tag--swatch.swatch {
  margin: 0;
  flex: 0;

  .swatch-element {
    span {
      display: block;
      border-radius: 50%;
    }
  }
  @include media-query ($medium-down) {
    display: inline-block;
    margin: 0;
  }
}

.collection__filter-tag--default {
  margin-bottom: 10px;
  padding-right: 15px;
  display: flex;
  align-items: center;
  justify-content: center;

  button {
    display: block;
    padding: 2px 0 0 25px;
    position: relative;
    text-align: left;
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    font-size: $font-size-base;

    &::before {
      content: '';
      position: absolute;
      left: 0;
      top: 50%;
      margin-top: -8px;
      width: 16px;
      height: 16px;
      border: 1px solid $color_site_border;
      background: #fff;
    }
  }

  &.is-active {
    button {
      &::before {
        content: '';
        color: #fff;
        border-color: #333;
        background: #333 url("{{ 'icon-check.svg' | asset_url }}") no-repeat center center;
        background-size: 10px auto;
      }
    }
  }
  @include media-query ($medium-down) {
    display: block;
    text-align: left;
  }
}

.collection__filter-tag--swatch.swatch {
  &.is-active {
    .swatch-element {
      label {
        &::after {
          opacity: 1;
          visibility: visible;
        }
      }
    }
  }

  .swatch-element {
    width: 60px;
    flex-direction: column;
    align-items: center;
    margin: 0 2px 10px;
    padding: 0;
    background: transparent;
    line-height: 1.2;

    label {
      float: none;
      display: block;
      width: $swatch_width;
      position: relative;
      padding: 0;
      margin: 5px auto;
    }

    &::after {
      position: static;
      margin-top: 4px;
      background-color: transparent;
      padding: 0;
      border-radius: 0;
      box-shadow: none;
      border: 0;
      opacity: 1;
      visibility: visible;
      font-size: 11px;
      line-height: 1.2;
      white-space: normal;
      @include prefix(transform, 'none', webkit moz ms o spec);
    }
  }
}

.collection__filter-tag--number {
  display: inline-block;
  padding: 0;
  margin: 0 6px 10px 0;

  button {
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0;
    padding: 6px 3px;
    height: 31px;
    min-width: 31px;
    border-radius: 16px;
    line-height: 17px;
    border: 1px solid $color_site_border;
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    font-size: $font-size-base;
  }

  &.is-active {
    button {
      color: #fff;
      border-color: #333;
      background: #333;
    }
  }
}

.collection__sorting {
  width: 200px;
  display: flex;
  align-items: flex-end;

  .js-dropdown {
    width: 200px;
  }
}
@include media-query($small) {
  .collection__sorting {
    width: 100%;

    .js-dropdown {
      width: 100%;
    }
  }
}
@include media-query($medium-up) {
  .collection__nav--sort {
    display: flex;
    justify-content: space-between;
  }

  .collection__nav {
    background: $color_bg_secondary;
  }
}
