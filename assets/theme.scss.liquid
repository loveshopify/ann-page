.header__account__link{
  padding-top: 2px;
  font-size: 0.95em;
}

.search {
  @include prefix(transition, height 0.2s ease-in-out, webkit moz ms o spec);
  position: relative;
  height: 0;
  width: 100%;
  z-index: 1000;
  background: rgba(0,0,0,0);

  input:focus{
    outline: none;
  }
  input::placeholder {
    color: $color_header_link;
  }

  .search-query {
    @extend .p;
    padding: 0 80px 0 40px;
    height: 80px;
    color: $color_header_link;
    background: $color_header_bg;
    outline: none;
    margin: 0;
    border: 0;
    width: 100%;
    font-family: $font-stack-body;
    border-bottom: 1px solid $color_menu_border;

    @include media-query( $small ) {
      font-size: 16px;
      line-height: normal;
      white-space: nowrap;
    }
  }

  .remove-search {
    position: absolute;
    right: 25px;
    top: 25px;
    cursor: pointer;
    color: $color_header_bg;
    width: 30px;
    height: 30px;

    &:before,
    &:after {
      content: '';
      position: absolute;
      width: 18px;
      top: 50%;
      left: 6px;
      height: 1px;
      background: $color_header_link;
      @include prefix(transform, rotate(45deg), webkit moz spec);
      @include prefix(transition, background 0.2s ease-in-out, webkit moz ms o spec);

      &:hover {
        background: darken( $color_header_link, 10% );
      }
    }
    &:after {
      @include prefix(transform, rotate(-45deg), webkit moz spec);
    }
  }

  .search-results-text {
    padding-bottom: 0;
  }
}
.search--open {
  height: 80px;
  @include media-query( $small ) {
    height: 60px;
  }

  .search__form {
    height: 80px;
  }

  &.search--loading,
  &.search--loaded {
    .search__results {
      max-height: calc(100vh - 80px);
    }
  }
}

.search__form {
  height: 0;
  overflow: hidden;
  position: relative;
  @include prefix(transition, height 0.2s ease-in-out, webkit moz ms o spec);
}

.search__results {
  position: absolute;
  top: 100%;
  left: 0;
  right: 0;
  max-height: 0;
  overflow: hidden;
  background: $color_bg;
  box-shadow: 0 5px 10px rgba(0, 0, 0, .1);
  @include prefix(transition, max-height 0.2s ease-in-out, webkit moz ms o spec);

  .pagination {
    padding: 0;
  }
}

.loader--search {
  margin: 30px auto 60px;
  opacity: 0;
  visibility: hidden;
  transition: opacity .2s ease, visibility 0s linear .2s;

  .search--loading & {
    opacity: 1;
    visibility: visible;
    transition: opacity .2s ease, visibility 0s linear 0s;
  }

  .search--loaded & {
    display: none;
  }
}

.search__results-scroller {
  overflow: auto;
  max-height: calc(100vh - 80px);
}

.product-grid--results {
  .product-item{
    width: 25%;

    @include media-query($small) {
      width: 50%;
    }
  }
}

li.has-sub-nav > a .icon {
  margin-left: 9px;
  display: inline-block;
  vertical-align: middle;
  width: 6px;
  height: 10px;
  @include prefix(transform, rotate(90deg), webkit spec);
}

/**
 * DROPDOWNS
 */

/*================ Desktop ================*/
@include media-query($medium-up) {
  .nav__links{
    flex: 0 1 100%;
    display: flex;
    justify-content: center;
    z-index: 1400;
    border-top: 1px solid $color_menu_border;
    border-bottom: 1px solid $color_menu_border;
    background-color: $color_menu_bg;
    padding: 0;
    transition: background 0.2s ease-in-out, border-color 0.2s ease-in-out;
  }
  .main-menu {
    display: inline-block;
    & > li {
      display: inline-block;
      padding: 14px 5px 11px 5px;
      &:first-child > a {
        margin-left: 10px;
      }
      & > a {
        margin-left: 30px;
        padding: 14px 5px;
        white-space: nowrap;
        color: $color_menu_link;
        &:hover{
          color: $color_menu_link_hover;
        }
      }
    }
  }
  .nav-carat-small {
    padding-left: 8px;
  }
  .nav-carat-small svg{
    width: 5px;
    transform: rotateZ(0deg);
    transition: transform 0.25s ease-out;
    fill: $color_menu_link;
  }
  .main-menu > li:hover{
    & > a > .nav-carat-small svg{
      transform: rotateZ(90deg);
      fill: $color_menu_link_hover;
    }
  }

  .grandparent > .main-menu-dropdown > ul > .parent .nav-carat,
  .grandparent > .main-menu-dropdown > ul > .parent .nav-carat-small{
    display: none;
  }
  .nav-carat{
    display: none;
  }
  .main-menu a{
    text-decoration: none;
  }
  .main-menu-dropdown {
    text-align: left;
    ul{ list-style-type: none; }
    li{ padding-top: 8px; }
  }
  /*================ Large Dropdown ================*/
  .main-menu > .parent > .main-menu-dropdown{
    display: none;
    position: absolute;
    z-index: 5000;
    padding-top: 15px;
    background-color: $color_submenu_bg;
    border-bottom: 1px solid $color_menu_border;
    border-left: 1px solid $color_menu_border;
    border-right: 1px solid $color_menu_border;
    padding: 0 40px 20px 30px;
    margin-top: 11px;
    min-width: 130px;
    ul{
      margin: 0;
      padding: 0;
    }
    ul > li > a{
      color: $color_submenu_link;
      &:hover{
        color: $color_submenu_link_hover;
      }
    }
  }
  .main-menu > .parent:hover > .main-menu-dropdown,
  .main-menu > .parent.active > .main-menu-dropdown{
    display: block;
  }
  .main-menu > .parent:first-child > .main-menu-dropdown{
    margin-left: -30px;
  }

  /*================ Large MegaNav ================*/
  .nav{
    position: relative;
  }
  .grandparent > .main-menu-dropdown{
    display: none;
    top: 100%;
    position: absolute;
    background-color: $color_submenu_bg;
    top: calc(100% - 1px);
    width: 100%;
    max-height: calc(85vh - 110px);
    overflow-x: hidden;
    overflow-y: auto;
    left: 0;
    margin: 0;
    z-index: 5000;
    border-bottom: 1px solid $color_menu_border;
    padding-top: 30px;
    padding-bottom: 20px;

    a {
      color: $color_submenu_link;

      &.megamenu__image-title-link,
      &.megamenu__image-btn {
        color: #fff;
      }
    }
  }
  .grandparent:hover > .main-menu-dropdown,
  .grandparent.active > .main-menu-dropdown{
    display: block;
    .main-menu-dropdown{  display: block; }
  }
  .grandparent > .main-menu-dropdown ul{
    margin: 0 auto;
    max-width: $width-site;
    padding: 10px 30px 20px 30px;
    &:first-child{
      padding-left: 0;
    }
  }
  .grandparent > .main-menu-dropdown > ul > .parent > a,
  .grandparent > .main-menu-dropdown > ul > .child > a{
    width: 100%;
    display: inline-block;
    padding-bottom: 12px;
    border-bottom: 1px solid $color_menu_border;
    font-family: $font-stack-heading;
    font-style: $font-style-heading;
    font-weight: $font-weight-heading;
  }
  .grandparent .main-menu-dropdown a:hover,
  .grandparent .main-menu-dropdown .main-menu--active a{
    color: $color_submenu_link_hover !important;
  }
  .grandparent > a{
    position: relative;
  }

  /* catch the cast where a grandparent has a child */
  .grandparent > .main-menu-dropdown > ul > .parent,
  .grandparent > .main-menu-dropdown > ul > .child{
    float: left;
    min-height: 1px;
    padding-left: 30px;
    padding-top: 8px;
    vertical-align: top;
  }

  .grandparent > .main-menu-dropdown > ul > li {
    width: 33.3333%;
  }

  .grandparent {
    & > .main-menu-dropdown--megamenu {
      padding: 0;

      & > ul {
        max-width: 100%;
        padding: 0;
        display: flex;
        flex-wrap: wrap;

        & > li {
          padding-top: 0;
          width: 33.3333%;

          &:nth-child(3n+1){
            clear:both;
          }

          &.parent {
            padding-top: 8px;
          }
        }
      }
    }
  }
}

@include media-query($large-up) {
  .nav__links { padding: 0 80px; }

  .kids-2.grandparent > .main-menu-dropdown > ul > li,
  .kids-1.grandparent > .main-menu-dropdown > ul > li{
    width: 50%;
    &:nth-child(2n+1){
      clear:both;
    }
  }
  .kids-3.grandparent > .main-menu-dropdown > ul > li,
  .kids-6.grandparent > .main-menu-dropdown > ul > li,
  .kids-9.grandparent > .main-menu-dropdown > ul > li{
    width: 33.333%;
    &:nth-child(3n+1){
      clear:both;
    }
  }
  .kids-4.grandparent > .main-menu-dropdown > ul > li,
  .kids-7.grandparent > .main-menu-dropdown > ul > li,
  .kids-8.grandparent > .main-menu-dropdown > ul > li{
    width: 25%;
    &:nth-child(4n+1){
      clear:both;
    }
  }
  .kids-5.grandparent > .main-menu-dropdown > ul > li,
  .kids-10.grandparent > .main-menu-dropdown > ul > li{
    width: 20%;
    &:nth-child(5n+1){
      clear:both;
    }
  }

  .grandparent {
    & > .main-menu-dropdown--megamenu {
      & > ul {
        & > li {
          width: 20%;
        }
      }
    }
  }
}

.navigation__wrapper {
  position: relative;
}

/**
 * Mega menu
 */
.megamenu__image {
  height: 350px;
  max-height: 50vh;
  padding: 10px 15px;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  overflow: hidden;
}
.megamenu__image-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
}

.megamenu__image-title {
  position: relative;
  max-width: 100%;
  margin-bottom: 15px;
  text-align: center;

  a { color: inherit; }
}
.megamenu__image-btn {
  position: relative;
  display: inline-block;
  max-width: 100%;
  white-space: normal;
}

/**
 * Currency picker
 */
.header__currency {
  text-align: left;
  padding: 0 15px;

  @include media-query($medium-up) {
    position: absolute;
    top: 0;
    right: 10px;
    height: 100%;
    text-align: right;
  }

  .currency-picker {
    height: 100%;
    position: relative;
  }

  select.currency-picker {
    border: 0;
    font-size: 1em * $font-adjust-body;
    line-height: normal;
    padding: 0 15px 0 0;
    margin: 0;
    min-height: 100%;
    height: 46px;
    background-image: none;
    z-index: 5;
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    color: $color_menu_link;

    @include media-query($medium-down) {
      width: 100%;
    }
    &:focus{
      box-shadow: 0 2px $color_menu_border;
      color: $color_menu_link_hover;
    }
  }

  .selectArrow {
    position: absolute;
    top: 0;
    right: 0;
    height: 100%;
    width: 10px;
    display: flex;
    align-items: center;
    fill: $color_menu_link;
  }
}


.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(1){
  animation-duration: 0.1s;
}
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(2){
  animation-duration: 0.2s;
}
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(3){
  animation-duration: 0.3s;
}
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(4){
  animation-duration: 0.4s;
}
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(5),
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(6),
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(7),
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(8),
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(9),
.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent:nth-child(10){
  animation-duration: 0.5s;
}

.meganav--visible .grandparent > .main-menu-dropdown > ul > .parent{
  animation-name: expand;
  animation-timing-function: cubic-bezier(0,0,.5,.99);
}
@keyframes expand {
  0% {
    transform: translateY(-10px);
    opacity: 0;
  }
  100% {
    transform: translateX(0);
    opacity: 1;
  }
}

.meganav--visible .megamenu__image-bg{
  animation-name: scale-in-image;
  animation-timing-function: cubic-bezier(0,0,.5,.99);
  animation-duration: 0.4s;
}
@keyframes scale-in-image {
  0% {
    transform: scale(1.05);
    opacity: 0.75;
  }
  100% {
    transform: scale(1);
    opacity: 1;
  }
}

/*================ Touch ================*/
@include media-query($small) {
  .navigation__wrapper{
    display: none;
    position: absolute;
    width: 100%;
    max-height: calc(100vh - 46px);
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    background-color: $color_submenu_bg;
  }
  .navigation__wrapper.active{
    display: block;
  }
  .main-menu {
    margin: 0;
    padding: 0;
    list-style-type: none;
    text-align: left;
    a {
      color: $color_submenu_link;

      &.megamenu__image-title-link { 
        color: #fff; 
      }
      &.megamenu__image-btn {
        color: #fff;
      }
    }
    .main-menu--active a,
    a:hover{
      color: $color_submenu_link_hover;
    }
  }
  .main-menu li,
  .nav__account{
    position: relative;
    font-size: 17px * $font-adjust-body;
    font-weight: $font-weight-body;
  }
  .nav__account a,
  .nav-link{
    padding: 15px 30px 15px 15px;
    display: inline-block;
  }
  .main-menu-dropdown .nav-link{
    padding: 11px 30px 11px 15px;
    font-size: 14px * $font-adjust-body;
  }
  .nav-carat-small{
    display: none;
  }
  .nav-carat{
    position: absolute;
    right:0;
    top: 3px;
    padding: 10px 15px;
    transform: rotateZ(90deg);
    transition: transform 0.15s ease-out;
    display: flex;
    align-items: center;
    justify-content: center;

    svg{
      width: 15px;
      height: 15px;
      color: $color_submenu_link;
      fill: $color_submenu_link;
    }
    &:hover svg{
      color: $color_submenu_link_hover;
      fill: $color_submenu_link_hover;
    }
    &[aria-expanded="true"]{
      transform: rotateZ(270deg);
      svg{
        color: $color_submenu_link;
        fill: $color_submenu_link;
      }
    }
  }
  .main-menu-dropdown{
    display: none;

    &.expanded{
      display: block;
    }
  }
  .main-menu > li > .main-menu-dropdown {
    border-top: 1px solid $color_menu_border;
    border-bottom: 1px solid $color_menu_border;
    .nav-carat{
      top: 0
    }
  }
  .main-menu-dropdown .main-menu-dropdown{
    margin-left: 15px;
    border-left: 2px solid #f3f3f3;
  }
}

@include media-query($large-up) {
  .search {
    .search-query {
      padding: 0 70px 0 30px;
      height: 80px;
    }
  }
}

@include media-query($small) {
  .desktop {
    display: none;
  }

  a.toggle-menus {
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    width: 48px;
    transform: rotate(0deg);
    cursor: pointer;
    overflow: hidden;
    z-index: 9999;
  }

  .toggle-menus-lines {
    display: block;
    position: absolute;
    top: 50%;
    right: 7px;
    margin: -12px 0 0;
    width: 34px;
    height: 24px;
    transition: .5s ease-in-out;

    span {
      display: block;
      position: absolute;
      height: 3px;
      width: 50%;
      background: $color_header_link;
      opacity: 1;
      transform: rotate(0deg);
      transition: .25s ease-in-out;

      &:nth-child(even) {
        left: calc(50% - 7px);
        border-radius: 0 3px 3px 0;
      }

      &:nth-child(odd) {
        left: 7px;
        border-radius: 3px 0 0 3px;
      }

      &:nth-child(1),
      &:nth-child(2) {
        top: 3px;
      }

      &:nth-child(3),
      &:nth-child(4) {
        top: 10px;
      }

      &:nth-child(5),
      &:nth-child(6) {
        top: 17px;
      }
    }
  }

  a.toggle-menus--open {
    span {
      background: $color_header_link;

      &:nth-child(1),
      &:nth-child(6) {
          transform: rotate(45deg);
      }

      &:nth-child(2),
      &:nth-child(5) {
          transform: rotate(-45deg);
      }

      &:nth-child(1) {
          left: 7px;
          top: 9px;
      }

      &:nth-child(2) {
          left: calc(50% - 7px);
          top: 9px;
      }

      &:nth-child(3) {
          opacity: 0;
      }

      &:nth-child(4) {
          opacity: 0;
      }

      &:nth-child(5) {
          left: 7px;
          top: 12px;
      }

      &:nth-child(6) {
          left: calc(50% - 7px);
          top: 12px;
      }
    }
  }

  .header-content {
    z-index: 5;
  }

  .user-links {
    left: 15px;
    justify-content: flex-start;

    a {
      margin: 0 16px 0 0;

      &.user-link--icon {
        font-size: 0;
        line-height: 0;
        text-indent: -100%;
        overflow: hidden;
        position: relative;
        display: inline-block;
        height: 22px;
      }
    }
  }

  #SearchToggle {
    order: 1;
  }
  #account-link {
    order: 2;
  }

  #customer_logout_link {
    order: 3;
  }

  #account-links {
    display: none;
  }

  #CartButton {
    position: absolute;
    right: 48px;
    padding: 0 10px;
    margin: 0;
  }

  .touch .header-content a.toggle-menus {
    @include prefix(transition, right 0.4s ease-in-out, webkit moz ms o spec);
  }

  // SEARCH
  .search {
    .search-query {
      height: 60px;
      padding: 0 40px 0 15px;
    }

    .remove-search {
      right: 15px;
      top: 15px;
    }
  }
}

#CartButton {
  @media screen and (max-width: 649px) {
    padding: 0;
  }
}

