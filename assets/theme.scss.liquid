        margin: 0;

        a {
          padding: 8px 10px;
          font-weight: normal;
          opacity: 1;
        }
      }

      .slick-arrow {
        margin: 0;
        top: 0;
        bottom: 0;
        color: $color_text;
        background: $color_bg;
        box-shadow: none;
        font-size: 21px;
        line-height: 1;
        display: flex;
        align-items: center;
        opacity: .7;
        @include prefix(transform, none, webkit moz ms o spec);
      }

      .slick-prev {
        left: 0;
        padding-left: 12px;

        svg {
          @include prefix(transform, rotateY(-180deg), webkit mz o spec);
        }
      }

      .slick-next {
        right: 0;
        padding-right: 12px;
      }
