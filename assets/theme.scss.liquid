.align--bottom-center{
  align-items: flex-end;
  justify-content: center;
  text-align: center;
}
.align--bottom-right{
  align-items: flex-end;
  justify-content: flex-end;
  text-align: right;
}

/*=== Hero Section Layout ==*/

.template-index .container{
  background-color: rgba(255,255,255,0);
}

.hero__content__wrapper{
  display: flex;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 200;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.hero__content{
  margin: 0 60px;
  padding: 60px;
  max-width: 60%;
  @include box_shadow(1, #000);
  @include media-query($medium) {
    max-width: 70%;
    padding: 45px;
    margin: 0 30px;
  }
  @include media-query($small) {
    max-width: 80%;
    padding: 30px;
    margin: 0 20px;
  }
}

.hero__button-image{
  max-width: 50%;
  padding: 60px;
  @include media-query($medium) {
    max-width: 70%;
    padding: 45px
  }
  @include media-query($small) {
    max-width: 80%;
    padding: 30px;
  }
  img{
    display: block;
    width: 100%
  }
}

.hero__logo {
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-top:  15px;
  margin-bottom: 15px;
  height: auto;
  max-width: 100%;
}

.hero__title{
  margin-top: 0;
  margin-bottom: rem(10px);
  @include media-query($small){
    font-size: 1.5em;
  }
}
.hero__description{
  margin-bottom: 1.5em;
  @include media-query($medium-down){
    margin-bottom: 1em;
  }

  p {
    font-size: inherit;
    font-style: inherit;
    font-weight: inherit;
    line-height: inherit;
    margin-bottom: inherit;
  }
}

.text-light{
  color: #fff;
  .image-overlay{
    background-color: #000;
  }
  .hero__content{
    background-color: $color_text_dark;
  }
  .hero__btn{
    @extend .btn--white;
  }
}
.text-dark{
  .image-overlay, .hero__content{
    background-color: #FFFFFF;
  }
  .hero__btn{
    @extend .btn--neutral;
  }
}
.hero__content--transparent .hero__content{
  background-color: rgba(255,255,255,0);
  box-shadow: none;
}

/*=============== Index Columns ===============*/

.index-columns{
  padding-top: 30px;
  padding-bottom: 30px;
  @include media-query($medium) {
    padding-top: 20px;
    padding-bottom: 20px;
  }
  @include media-query($small) {
    padding-top: 15px;
    padding-bottom: 15px;
  }
}

.column__grid__item{
  padding-bottom: 30px;
  @include media-query($medium) {
    padding-bottom: 15px;
  }
}

.wrapper--columns {
  max-width: none;
  margin: 0 30px;
  @include media-query($medium) {
    margin: 0 20px;
  }
  @include media-query($small) {
    margin: 0 15px;
  }
}

@include media-query($small) {
  .index-columns .grid__item:not(:last-child) a.btn{
    margin-bottom: 40px;
  }
}
.column__heading{
  margin-top: rem(20px);
  margin-bottom: rem(3px);;
}
.column__text p{
  margin-top: 10px;
}

.column__image {
  display: block;
  height: 0;
  position: relative;
  overflow: hidden;
}

.column__image__bg{
  position: absolute;
  display: block;
  top: -1px;
  height:100%;
  width: 100%;
  overflow: hidden;
  background-size: cover;
  background-position: center center;
}

.column__image__wrapper{
  position: relative;
}
.column__image__link{
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
}

.icon__play--column{
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
