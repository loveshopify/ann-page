.look__dot__wrapper{
  position: absolute;
}

.look__dot{
  position: absolute;
  z-index: 100;
  width: 15px;
  height: 15px;
  background-color: #fff;
  border-radius: 10px;
  &::after{
    content: "";
    width: 25px;
    height: 25px;
    background-color: inherit;
    border-radius: 25px;
    opacity: 0.3;
    position: absolute;
    top: -5px;
    left: -5px;
  }
  &::before{
    content: "";
    width: 25px;
    height: 25px;
    background-color: inherit;
    border-radius: 25px;
    opacity: 0.3;
    position: absolute;
    top: -5px;
    left: -5px;
    animation: pulse 2.5s linear;
    animation-iteration-count: infinite;
  }
}

@keyframes pulse {
 0% {
    transform: scale(0);
    opacity: 0.0;
 }
 89% {
    transform: scale(0);
    opacity: 0;
 }
 90% {
    transform: scale(1);
    opacity: 0.2;
 }
 100% {
    transform: scale(5);
    opacity: 0.0;
 }
}

