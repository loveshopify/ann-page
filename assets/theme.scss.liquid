  [data-aos="img-in"] {
    opacity: 0;
    transform: scale(1.2);
    transition-property: transform, opacity;

    &.aos-animate {
      opacity: 1;
      transform: scale(1);
    }
  }

  [data-aos="svg-dash"] path{
    stroke-dasharray: 100;
    stroke-dashoffset: 100;
  }
  [data-aos="svg-dash"].aos-animate path{
    animation: dash 1.5s linear forwards 1;
  }
}

@keyframes dash {
  from {
    stroke-dashoffset: 100;
  }
  to {
    stroke-dashoffset: 0;
  }
}

html.iframe{
  @include media-query($small) {
    [data-aos]{
      opacity: 1 !important;
      transform: none !important;
    }
  }
