/*================ Cart popdown ================*/
.cart-popover {
  position: fixed;
  right: 0;
  z-index: 600;
  width: 50%;
  max-width: 100%;
  opacity: 0;
  visibility: hidden;
  transition: opacity .5s ease, transform .5s ease, visibility 0s linear .5s;
  text-align: center;
  transform: translate3d(0, -100%, 0);
  &.contains-errors{
    position: static !important;
    width: 100%;
    padding-bottom: 20px;
  }
  &.is-visible {
    opacity: 1;
    visibility: visible;
    transition: opacity .5s ease, transform .5s ease, visibility 0s linear 0s;
    transform: translate3d(0, 0, 0);
  }
  @include media-query ( $medium ) {
    width: 75%;
  }
  @include media-query ( $small ) {
    &.is-visible{
      position: static !important;
      top: 0 !important;
    }
    width: 100%;
    padding-bottom: 20px;
  }

  h3 {
    font-size: 18px;
    line-height: 1.45;
  }

  h6 {
    text-transform: uppercase;
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    font-size: 12px;
    letter-spacing: 0.1em;
    margin-top: 32px;
    margin-bottom: 5px;
  }

  p {
    font-size: 12px;
    line-height: 1.2;
  }

  .buttons-wrap {
    margin-top: 30px;
  }

  .btn {
    display: block;
    width: 100%;
    margin-bottom: 6px;
    font-size: 13px;
  }

  .product__variant{
    font-style: italic;
  }

  .close {
    position: absolute;
    top: 4px;
    right: 12px;
    width: 20px;
    height: 20px;
    padding: 5px;
    z-index: 20;
    cursor: pointer;

    .icon {
      stroke: #333;
      width: 100%;
      height: 100%;
      pointer-events: none;
    }
  }

  .errors {
    max-width: 100%;
    margin: 0;
    padding: 24px 30px;
    @include media-query( $small ) {
      padding: 15px 20px;
    }

    .close {
      .icon {
        stroke: #721c24;
      }
    }
  }
}

