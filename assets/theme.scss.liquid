  .account-sidebar--mobile {
    display: block;
    padding: 15px;
    cursor: pointer;
    position: relative;
  }
  .account-sidebar--mobile:after {
    font-family: 'Icons';
    content: '\e02d';
    position: absolute;
    right: 15px;
    font-size: 75%;
    top: 50%;
    z-index: 10;
    @include prefix(transform, translateY(-50%), webkit moz ms o spec);
  }
  .account-sidebar ul {
    position: absolute;
    z-index: 999;
    background: $color_bg_secondary;
    top: 100%;
    left: 0;
    right: 0;
    max-height: 0;
    overflow: hidden;
    -webkit-transition: max-height 0.5s ease-in;
    -moz-transition: max-height 0.5s ease-in;
