  margin: 0 0 20px;
}
.show-note:before,
.show-calculator:before {
  content: '+';
  display: inline-block;
  width: 16px;
}
.show-note.open:before,
.show-calculator.open:before {
  content: '\2013';
  display: inline-block;
  width: 16px;
}
#ShippingCalculator {
  border: 1px solid $color_site_border;
  padding: 20px;

  p {
    margin-bottom: 1em;
  }

  label {
    display: block;
    margin-bottom: 5px;
    font-size: 0.875em;
    line-height: 1.2;
  }
  input,
  select {
    margin: 0;
    width: 100% !important;
  }

  .btn {
    margin: 15px 0 0;
    white-space: normal;
  }

  p#shipping-rates-feedback {
    font-style: italic;
  }

  #wrapper-response {
    margin-top: 20px;
  }
}

.error,
p.error,
label.error {
  color: #bb0000;
}

#shipping-rates{
  list-style-type: none;
  margin: 0;
  padding: 0;
  border-top: 1px solid $color_site_border_light;
  li{
    border-bottom: 1px solid $color_site_border_light;
    padding: 15px 0;
  }
}

@media screen and (max-width: 768px) {
  .cart table tbody td img {
    max-width: 75px;
  }
}
@media screen and (max-width: 767px) {
  .cart table tbody td img {
    max-width: 75px;
  }
  #CartOuter,
  .cart-info-container {
    width: 100%;
  }
  .cart-info-container {
    text-align: left;

    .proceed,
    .cart-section-options {
      display: inline-block;
      width: 48%;
      vertical-align: top;
    }

    .cart-extra { margin: 10px 0 0; }
    .btn { max-width: 262px; }    
    small.additional-checkout { width: 262px; }
  }
  .additional-checkout-buttons { 
    width: 262px; 

    [data-shopify-buttoncontainer] > div {height: auto !important; }
  }
}
@media screen and (max-width: 639px) {
  .cart {
    margin: 20px auto;

    .remove { display: none; } 
  }

  .info__subtotal { text-align: right; }
  .info__tax { 
    text-align: center; 
    margin-bottom: 10px;
  }
  .cart-info-container {
    .proceed,
    .cart-section-options {
      width: 100%;
      text-align: center;
    }
    small.additional-checkout { width: 100%; }
  }
  .additional-checkout-buttons {
    margin-left: auto;
    margin-right: auto;
  }
}
@media screen and (max-width: 480px) {
  .cart {
    table { margin-top: 0; }
    table th,
    .desc,
    .price {
      display: none;
    }

    .update {
      padding: 10px 0;
      .btn { margin: 0; }
    }

    table tbody tr:first-child > td { border-top: 1px solid $color_site_border; }
  }
    
  .cart-info-container {
    .proceed,
    .cart-section-options {
      margin: 0 0 20px;
    }
  }
